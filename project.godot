; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

[addons]

node_library/enable_screen_transition=true

[application]

config/name="LD51"
run/main_scene="res://scenes/Main.tscn"
config/icon="res://icon.png"

[autoload]

ScreenTransition="*res://addons/node_library/scenes/ScreenTransition.tscn"
MusicPlayer="*res://scenes/autoload/MusicPlayer.tscn"
Visuals="*res://scenes/autoload/Visuals.tscn"

[debug]

settings/fps/force_fps=240
gdscript/warnings/return_value_discarded=false

[display]

window/size/width=1280
window/size/height=720
window/size/test_width=1920
window/size/test_height=1080
window/stretch/mode="viewport"
window/stretch/aspect="keep"

[editor_plugins]

enabled=PoolStringArray( "res://addons/center_pivot_offset/plugin.cfg", "res://addons/node_library/plugin.cfg", "res://addons/retile/plugin.cfg" )

